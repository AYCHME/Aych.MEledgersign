{
  "name": "eosjs-ledger-signature-provider",
  "version": "0.1.2",
  "description": "A Signature provider for communicating with eosjs from a Ledger device.",
  "author": {
    "name": "block.one",
    "url": "https://www.block.one"
  },
  "contributors": [
    "Randy Torres",
    "Brandon Fancher"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "build": "rm -rf dist & tsc -p ./tsconfig.json",
    "test": "jest",
    "prepare": "yarn build",
    "docs": "typedoc --excludePrivate --excludeProtected --out ./docs --theme markdown"
  },
  "dependencies": {
    "@babel/core": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "@ledgerhq/hw-transport": "5.11.0",
    "@ledgerhq/hw-transport-u2f": "5.11.0",
    "@types/elliptic": "6.4.12",
    "@types/jest": "25.1.4",
    "asn1-ber": "1.0.9",
    "bip32-path": "0.4.2",
    "buffer": "5.5.0",
    "eosjs": "20.0.4-67ee21f.0",
    "elliptic": "6.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@blockone/eslint-config-blockone": "^3.0.0",
    "@types/node": "^11.9.4",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "tslib": "^1.11.1",
    "typedoc": "^0.16.11",
    "typedoc-plugin-markdown": "^2.2.17",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(tsx?)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testRegex": "(/src/.*(\\.|/)(test|spec))\\.(ts?|tsx?)$"
  }
}
